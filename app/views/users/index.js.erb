<% new_title = @index_title || e9_t(:index_title) %>
document.title = "<%= escape_javascript(meta_title(new_title)) %>";
$("h1.title").html("<%= escape_javascript(new_title) %>");

$(".users-pagination").html("<%= escape_javascript(will_paginate(collection)) %>");
$(".users").html(
<% if collection.empty? %>
  '<div class="no-records"><%= t(:no_users_found) %></div>'
<% else %>
  "<%= escape_javascript(render(collection)) %>"
<% end %>
);  
